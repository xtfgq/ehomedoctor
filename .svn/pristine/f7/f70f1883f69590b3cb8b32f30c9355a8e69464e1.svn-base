<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/base_color_text_white"
    android:orientation="vertical">

    <include layout="@layout/layout_toolbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/layout_base_file"
            android:layout_width="match_parent"
            android:layout_height="52dp">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:textSize="16sp"
                android:textColor="@color/base_color_text_black"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="基础档案"
                />
            <ImageView

                android:layout_width="10dp"
                android:layout_height="22dp"
                android:src="@mipmap/icon_arrow_r"
                android:layout_marginRight="4dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_line_text" />
        <RelativeLayout
            android:id="@+id/layout_health_data"
            android:layout_width="match_parent"
            android:layout_height="52dp">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:textSize="16sp"
                android:textColor="@color/base_color_text_black"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="健康数据"
                />
            <ImageView

                android:layout_width="10dp"
                android:layout_height="22dp"
                android:src="@mipmap/icon_arrow_r"
                android:layout_marginRight="4dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_line_text" />
        <RelativeLayout
            android:id="@+id/layout_jzjl"
            android:layout_width="match_parent"
            android:layout_height="52dp">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:textSize="16sp"
                android:textColor="@color/base_color_text_black"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="就诊记录"
                />
            <ImageView
                android:layout_width="10dp"
                android:layout_height="22dp"
                android:src="@mipmap/icon_arrow_r"
                android:layout_marginRight="4dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_line_text" />
        <RelativeLayout
            android:id="@+id/layout_xdbg"
            android:layout_width="match_parent"
            android:layout_height="52dp">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:textSize="16sp"
                android:textColor="@color/base_color_text_black"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="心电报告"
                />
            <ImageView
                android:layout_width="10dp"
                android:layout_height="22dp"
                android:src="@mipmap/icon_arrow_r"
                android:layout_marginRight="4dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_line_text" />
        <RelativeLayout
            android:id="@+id/layout_jcbg"
            android:layout_width="match_parent"
            android:layout_height="52dp">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:textSize="16sp"
                android:textColor="@color/base_color_text_black"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="检查报告"
                />
            <ImageView
                android:layout_width="10dp"
                android:layout_height="22dp"
                android:src="@mipmap/icon_arrow_r"
                android:layout_marginRight="4dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_line_text" />

    </LinearLayout>
</LinearLayout>
